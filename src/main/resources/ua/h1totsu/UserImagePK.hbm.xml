<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ua.h1totsu.entity.UserImagePK" table="user_image" catalog="imagedb">
        <composite-id mapped="true" class="ua.h1totsu.entity.UserImagePK">
            <key-property name="userId" column="user_id"/>
            <key-property name="imageId" column="image_id"/>
        </composite-id>
    </class>
</hibernate-mapping>